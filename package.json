{
  "name": "mmps-new",
  "version": "1.0.0",
  "description": "Express TypeScript application",
  "main": "dist/index.js",
  "type": "commonjs",
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "dev:build": "tsc --watch",
    "dev:debug": "tsx watch --inspect src/index.ts",
    "debug": "node --inspect dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepare": "husky"
  },
  "keywords": [
    "express",
    "typescript",
    "node"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@langchain/anthropic": "^1.1.3",
    "@langchain/core": "^1.1.0",
    "@langchain/langgraph": "^1.0.2",
    "@langchain/openai": "^1.1.3",
    "@modelcontextprotocol/sdk": "^1.23.0",
    "@modelcontextprotocol/server-github": "^2025.4.8",
    "@pinecone-database/pinecone": "^6.1.3",
    "@vitalets/google-translate-api": "^9.2.1",
    "axios": "^1.13.2",
    "canvas": "^3.2.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "googleapis": "^166.0.0",
    "langchain": "^1.1.1",
    "mongodb": "^7.0.0",
    "node-cron": "^4.2.1",
    "node-telegram-bot-api": "^0.66.0",
    "openai": "^6.9.1",
    "telegram": "^2.26.22",
    "twilio": "^5.10.6",
    "yahoo-finance2": "^3.10.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/node-telegram-bot-api": "^0.64.13",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "semantic-release": "^25.0.2",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
